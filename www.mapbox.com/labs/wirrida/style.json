{
  "version": 4,
  "sprite": "sprite",
  "transition": {
    "duration": 900,
    "delay": 0
  },
  "sources": {
    "mapbox": {
      "type": "raster",
      "url": "mapbox://mapbox.satellite",
      "tileSize": 256
    }
  },
  "layers": [{
    "id": "background",
    "style": {
      "background-color": "rgb(170, 73, 55)"
    },
    "type": "background"
  }, {
    "id": "satellite",
    "source": "mapbox",
    "type": "raster"
  }, {
    "id": "places-borders",
    "source": "places",
    "type": "symbol",
    "render": {
      "icon-ignore-placement": true,
      "icon-image": "dot.sdf",
      "icon-max-size": 0,
      "opacity": 0
    }
  }]
}
