{
  "version": 6,
  "sources": {
    "streets": {
      "type": "vector",
      "url": "mapbox://mapbox.mapbox-streets-v6-dev,mapbox.mapbox-terrain-v1"
    }
  },
  "sprite": "assets/sprite",
  "glyphs": "mapbox://fontstack/{fontstack}/{range}.pbf",
  "layers": [{
    "id": "background",
    "type": "background",
    "paint": {
      "background-color": "rgba(0,0,0,0)"
    }
  }, {
    "id": "water",
    "type": "fill",
    "source": "streets",
    "source-layer": "water",
    "paint": {
      "fill-color": "rgba(0,0,0,0.06)"
    }
  }, {
    "id": "landcover",
    "type": "fill",
    "source": "streets",
    "source-layer": "landcover",
    "paint": {
      "fill-color": "rgba(255,255,255,0.01)"
    }
  }, {
    "id": "hillshade_full",
    "type": "fill",
    "source": "streets",
    "source-layer": "hillshade",
    "filter": ["==", "class", "full_shadow"],
    "paint": {
      "fill-color": "rgba(0,0,0,0.02)"
    }
  }, {
    "id": "hillshade_medium",
    "type": "fill",
    "source": "streets",
    "source-layer": "hillshade",
    "filter": ["==", "class", "medium_shadow"],
    "paint": {
      "fill-color": "rgba(0,0,0,0.015)"
    }
  }, {
    "id": "contour_main",
    "type": "line",
    "source": "streets",
    "source-layer": "contour",
    "filter": ["in", "index", 5, 10],
    "paint": {
      "line-width": 2,
      "line-color": "rgba(0,0,0,0.065)"
    },
    "paint.over": {
      "line-color": "rgba(0,0,0,0.15)"
    }
  }, {
    "id": "contour_other",
    "type": "line",
    "source": "streets",
    "source-layer": "contour",
    "filter": ["!in", "index", 5, 10],
    "paint": {
      "line-width": 1,
      "line-color": "rgba(0,0,0,0.06)"
    },
    "paint.over": {
      "line-color": "rgba(0,0,0,0.14)"
    }
  }, {
    "id": "road",
    "type": "line",
    "source": "streets",
    "source-layer": "road",
    "paint": {
      "line-width": 1,
      "line-color": "rgba(0,0,0,0.07)"
    }
  }, {
    "id": "tunnel",
    "type": "line",
    "source": "streets",
    "source-layer": "tunnel",
    "paint": {
      "line-width": 1,
      "line-color": "rgba(0,0,0,0.07)"
    }
  }, {
    "id": "bridge",
    "type": "line",
    "source": "streets",
    "source-layer": "bridge",
    "paint": {
      "line-width": 1,
      "line-color": "rgba(0,0,0,0.07)"
    }
  }, {
    "id": "city_dot",
    "type": "symbol",
    "source": "streets",
    "source-layer": "place_label",
    "filter": ["all", ["==", "type", "city"], ["==", "$type", "Point"]],
    "layout": {
      "icon-image": "dot.sdf",
      "icon-max-size": 8,
      "icon-allow-overlap": true,
      "icon-ignore-placement": true
    },
    "paint": {
      "icon-size": {
        "fn": "stops",
        "stops": [[0, 2], [22, 8]]
      },
      "icon-color": "rgba(0,0,0,0.06)"
    }
  }, {
    "id": "state_dot",
    "type": "symbol",
    "source": "streets",
    "source-layer": "state_label",
    "filter": ["==", "$type", "Point"],
    "layout": {
      "icon-image": "dot.sdf",
      "icon-max-size": 14,
      "icon-allow-overlap": true,
      "icon-ignore-placement": true
    },
    "paint": {
      "icon-size": {
        "fn": "stops",
        "stops": [[0, 4], [22, 14]]
      },
      "icon-color": "rgba(0,0,0,0.03)"
    }
  }, {
    "id": "poi_triangle",
    "type": "symbol",
    "source": "streets",
    "source-layer": "poi_label",
    "filter": ["==", "$type", "Point"],
    "layout": {
      "icon-image": "triangle.sdf",
      "icon-max-size": 4,
      "symbol-placement": "point"
    },
    "paint": {
      "icon-size": {
        "fn": "stops",
        "stops": [[0, 2], [22, 4]]
      },
      "icon-color": "rgba(0,0,0,0.1)"
    }
  }, {
    "id": "contour_label",
    "type": "symbol",
    "source": "streets",
    "source-layer": "contour",
    "filter": ["==", "index", 10],
    "layout": {
      "text-font": "Open Sans Semibold, Arial Unicode MS Regular",
      "text-field": "{ele}m",
      "text-max-size": 11,
      "text-offset": [0, -0.9],
      "symbol-placement": "line"
    },
    "paint": {
      "text-size": 11,
      "text-halo-width": 2,
      "text-color": "rgba(0,0,0,0.12)"
    },
    "paint.over": {
      "text-color": "rgba(0,0,0,0.3)"
    }
  }, {
    "id": "building_dot",
    "type": "symbol",
    "source": "streets",
    "source-layer": "building",
    "layout": {
      "icon-image": "dot.sdf",
      "icon-max-size": 0.6,
      "symbol-placement": "point"
    },
    "paint": {
      "icon-size": 0.6,
      "icon-color": "rgba(255,255,255,0.07)"
    }
  }]
}
